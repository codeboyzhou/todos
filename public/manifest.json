{
  "manifest_version": 3,
  "version": "2022.7.15",
  "name": "Todo Task Manager",
  "description": "A browser extension for managing your to-do list, compatible with Google Chrome and Microsoft Edge",
  "icons": {
    "16": "icons/icon_16x16.png",
    "32": "icons/icon_32x32.png",
    "48": "icons/icon_48x48.png",
    "128": "icons/icon_128x128.png"
  },
  "action": {
    "default_icon": {
      "128": "icons/icon_128x128.png"
    },
    "default_popup": "popup/popup.html"
  }
}
